{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "check-dist": {
      "dependsOn": ["^check-dist"]
    },
    "package": {
      "dependsOn": ["^package"],
      "outputs": ["dist/**", "layer/**"]
    }
  }
}
